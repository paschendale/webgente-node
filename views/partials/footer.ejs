<!-- Redimensiona a div do mapa para o espaço vertical disponivel em tela e habilita/desabilita botão da legenda com base no tamanho da tela-->
<script type="application/javascript">
    function resizeMap () {
        $("#map").height(window.innerHeight)
        // var map = document.getElementById('map');
        // var newHeight = window.innerHeight - document.getElementById('navbar').clientHeight;
        // map.style.height = newHeight + "px";
    }

    function disableLegendContainer () {
        if ($(window).width() < 1000) {
            document.getElementById('legend-button').click();
        } 
    }

    $(document).ready(disableLegendContainer);
    $(window).resize(disableLegendContainer);
    
     $(document).ready(resizeMap);
     $(window).resize(resizeMap);    
    </script>

